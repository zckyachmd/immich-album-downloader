{
  "name": "immich-album-downloader",
  "version": "1.0.3",
  "type": "module",
  "scripts": {
    "download": "node main.js",
    "download:all": "node main.js --all",
    "download:resume": "node main.js --resume-failed",
    "download:dry": "node main.js --dry-run",
    "download:verbose": "node main.js --verbose",
    "format": "prettier --write \"**/*.{js,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,json,md,yml,yaml}\"",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "dependencies": {
    "better-sqlite3": "^11.10.0",
    "dotenv": "^16.6.1",
    "inquirer": "^9.3.8",
    "node-fetch": "^3.3.2",
    "p-limit": "^4.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "prettier": "^3.7.4"
  }
}
